<template v-for="item in items">
  <a :href="item.link"
	class="w-10 h-10 flex items-center justify-center border select-none"
		:class="{
			'bg-[#141414] border-[#2a2a2a] font-semibold cursor-default': type(item.is_current) == 'bool' && item.is_current,
			'bg-[#1b1b1b] border-[#2a2a2a] text-gray-300 hover:bg-[#2a2a2a]': type(item.is_current) == 'bool' && !item.is_current
		}">

	{{ item.page }}
  </a>
</template>
